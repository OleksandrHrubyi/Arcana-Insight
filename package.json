{
  "name": "arcana",
  "version": "0.0.1",
  "description": "tarot horoscope",
  "productName": "Arcana Insight",
  "author": "Oleksandr H BO:FE <oleksandr.h@lineir.me>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "iosbuild": "npx quasar build",
    "ioscopy": "npx cap copy ios",
    "iosopen": "open ios/App/App.xcworkspace",
    "iosprep": "npm run iosbuild && npm run ioscopy && npm run iosopen",
    "iossync": "npx cap sync ios && npm run iosopen",
    "ios:assets": "capacitor-assets generate --ios",
    "ios:sync": "npx cap sync ios",
    "ios:open": "npx cap open ios",
    "party": "npm run build && npm run iosbuild && npm run ioscopy && npm run iossync"
  },
  "dependencies": {
    "@capacitor-community/apple-sign-in": "^5.0.0",
    "@capacitor/core": "^5.7.8",
    "@capacitor/haptics": "^5.0.8",
    "@capacitor/ios": "^5.7.8",
    "@capacitor/preferences": "^5.0.8",
    "@capacitor/status-bar": "^5.0.8",
    "@fontsource/nunito": "^5.2.7",
    "@pixi/particle-emitter": "^5.0.8",
    "@quasar/extras": "^1.16.4",
    "@supabase/supabase-js": "^2.76.0",
    "gsap": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "pixi.js": "^8.14.0",
    "quasar": "^2.16.0",
    "vue": "3.5.20",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^5.7.8",
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.10.3",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
